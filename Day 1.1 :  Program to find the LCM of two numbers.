#include <stdio.h>
int main()
{
    int a, b, L;
    printf("Enter the value of A : ");
    scanf("%d", &a);
    printf("Enter the value of B : ");
    scanf("%d", &b);
   // L = a>b?a:b;
    for (L = a>b?a:b; L <= a * b; L++)

        if (L % a == 0 && L % b == 0)
            break;

    printf("LCM of A and B is %d", L);
return 0;

}
