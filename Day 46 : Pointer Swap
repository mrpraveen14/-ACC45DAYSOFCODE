//pointer swap 

#include<stdio.h>

void swap(int *a, int *b);

int main (){

    int x=4,y=10;

    printf("Before x : %d  y : %d",x,y);

    swap(&x,&y);

    printf("\n");

    printf("After x : %d  y : %d",x,y);

}

void swap(int *a, int *b){

    int temp;

    temp=*a;

    *a=*b;

    *b=temp;

}
