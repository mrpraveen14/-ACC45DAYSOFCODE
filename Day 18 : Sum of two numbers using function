//Day 18 : Sum of two numbers using function
#include<stdio.h>
int add (int x, int y);
int main(){
    int a,b,sum;
    printf("ENter the value of a and b : ");
    scanf("%d %d", &a, &b);
    sum = add (a,b);
    printf("Sum of a and b is : %d ", sum);
    return 0;
}
int add (int x, int y)
{
    int d;
    d=x+y;
    return (d) ;

}
