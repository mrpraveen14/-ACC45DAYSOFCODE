#include<stdio.h>

#define max 100

int main(){

    int a[max], n,sum=0;

    printf("Enter the size of Array : ");

    scanf("%d", &n);

    for(int i =0;i<n;i++){

        printf("Enter a[%d] element of array : ",i);

        scanf("%d",&a[i]);

    }

    for(int i =0;i<n;i++){

        if(a[i]<=5){

            sum = sum + i;

        }

    }

    printf("Sum of Index values : %d",sum);

    return 0;

}
