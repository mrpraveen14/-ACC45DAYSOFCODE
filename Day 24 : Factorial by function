#include <stdio.h>
int fact();
int main()
{
    int n;
    printf("Factorial of Which number you want : ");
    scanf("%d", &n);
    fact(n);
    return 0;
}
int fact(int a)
{
    int factorial = 1;
    for (int i = 1; i <= a; i++)
    {
        factorial *= i;
    }
    printf("Factorial of %d is %d", a, factorial);
}
