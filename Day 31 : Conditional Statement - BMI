#include<stdio.h>
int main(){
    int x,y,z;
    printf("Enter your Weight : ");
    scanf("%d",&x);
    printf("Enter value of Y : ");
    scanf("%d",&y);
    printf("Enter value of Z : ");
    scanf("%d",&z);

    if(x>y && x<z)
    printf("FIT");
    else if(x<y)
    printf("UNDERWEIGHT");
    else if(x>z)
    printf("OVERWEIGHT");
    return 0;
}
